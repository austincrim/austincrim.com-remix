{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "prebuild": "npm run css:build",
    "build": "remix build",
    "dev": "railway run npm run watch",
    "watch": "concurrently \"remix dev\" \"npm run css:watch\" ",
    "css:watch": "tailwindcss -i app/styles/in.css -o app/styles/tailwind.css --watch",
    "css:build": "NODE_ENV=production tailwindcss -i app/styles/in.css -o app/styles/tailwind.css",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@prisma/client": "^3.4.2",
    "@remix-run/react": "^0.20.1",
    "@remix-run/serve": "^0.20.1",
    "mdx-bundler": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6.0.0-beta.8",
    "rehype-highlight": "^5.0.0",
    "remix": "^0.20.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.20.1",
    "@tailwindcss/typography": "^0.4.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^6.4.0",
    "prisma": "^3.4.2",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
